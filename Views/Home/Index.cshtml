@{
    ViewBag.Title = "Home Page";
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body><br /><br /><br /><br /><br />
    <div class="container">
        <div class="col-md-6">
            <div class="form-group">
                <select id="comboboxIA" class="form-control">
                    <option value="-1">Selectionner l'IA</option>
                    @foreach (var ia in ViewBag.ias)
                    {
                        <option value="@ia.Id">@ia.NomIA</option>
                    }
                </select>
            </div>
            <div class="form-group">
                <select id="comboboxIEF" class="form-control"></select>
            </div>
            <div class="form-group">
                <select id="comboboxEtab" class="form-control"></select>
            </div>
        </div>


        <div class="col-md-6">
            <h2>Select2</h2>
            <select id="dynamicAttributes" multiple="true" data-tags="true">
                <option value="1">A</option>
                <option value="2">B</option>
                <option value="3">C</option>
            </select>
        </div>

        
    </div>
</body>
</html>